{
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"moduleResolution": "node",
		"inlineSourceMap": true,
		"sourceRoot": "/src",
		"outDir": "/out",
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"lib": [
			"es5", "es2015.generator", "es2015.iterable", "es2015.collection", "es2015.core"
		]
	}
}
